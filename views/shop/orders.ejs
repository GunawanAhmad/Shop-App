<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if (orders.length <= 0) { %>
            <h1>Nothing there!</h1>
            <% } else { %>
                <ul>
                    <% orders.products.forEach( order=> { %>
                        <li>
                            <h1><%= order.id%></h1>
                            <ul>
                                <% order.CartItem.forEach( item => { %>
                                <li><%= item.title%> (<%= item.OrderItem.quantity %>)</li>
                                <% }) %>
                            </ul>
                        </li>
                    <% }) %>
                </ul>
            <% }  %>
        </main>
        <%- include('../includes/end.ejs') %>